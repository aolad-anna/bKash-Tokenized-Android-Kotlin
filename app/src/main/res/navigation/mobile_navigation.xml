<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.bkash_tokenized_android_kotlin.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.bkash_tokenized_android_kotlin.ui.search.SearchFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_search" />

    <dialog
        android:id="@+id/webViewDialog"
        android:name="com.example.bkash_tokenized_android_kotlin.ui.home.WebViewFragment"
        tools:layout="@layout/fragment_web_view"/>

    <dialog
        android:id="@+id/bottomSheetDialog"
        android:name="com.example.bkash_tokenized_android_kotlin.ui.home.BottomSheetFragment"
        tools:layout="@layout/bottom_sheet_dialog"/>

</navigation>